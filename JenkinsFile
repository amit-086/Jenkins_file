pipeline{
        agent any
        stages {
                stage('Git-Checkout') {
                    steps {
                        echo 'checking out from git-repo';
                        git branch: 'main', url: 'https://github.com/amit-086/PipelineScript.git'
                    }
                }
                stage('Build') {
                    steps {
                        echo 'Building the project!';
                        bat 'mvn package'
                    }
                }   
             }
      }
